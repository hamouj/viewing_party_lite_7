<body>
<% if current_user %>
  <%= link_to "Log Out", session_path(current_user.id), method: :delete %>
<% else %>
  <nav id="create_new_user">
    <p><%= button_to "Register as a User", register_path, method: :get, class: 'button' %></p>
  </nav>

  <nav id="login">
    <br><%= link_to "Log In", new_session_path %>
  </nav>
<% end %>

  <section id="existing_users">
    <h3>Existing Users</h3>
    <% @users.each do |user| %>
      <div id="user-<%= user.id %>">
        </p><%= link_to "#{user.email}", "/users/#{user.id}" %></p>
      </div>
    <% end %>
  </section>
</body>
